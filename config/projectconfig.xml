<?xml version="1.0" encoding="UTF-8"?>
<configuration>
  <setting name="STARTUP_FILE" type="java.lang.String">./config/versions.xml</setting>

  <setting name="EXPORT_ORDER" type="java.util.List" contentType="de.comlet.releasefab.library.model.CCLTuple">
    <item>
      <first type="java.lang.String">VERSION</first>
      <second type="java.lang.Boolean">true</second>
    </item>
    <item>
      <first type="java.lang.String">IMPORTANT_INFORMATION</first>
      <second type="java.lang.Boolean">false</second>
    </item>
    <item>
      <first type="java.lang.String">GIT_COMMITS</first>
      <second type="java.lang.Boolean">true</second>
    </item>
  </setting>
  <setting name="VIEW_ORDER" type="java.util.List" contentType="de.comlet.releasefab.library.model.CCLTuple">
    <item>
      <first type="java.lang.String">VERSION</first>
      <second type="java.lang.Boolean">true</second>
    </item>
    <item>
      <first type="java.lang.String">IMPORTANT_INFORMATION</first>
      <second type="java.lang.Boolean">false</second>
    </item>
    <item>
      <first type="java.lang.String">GIT_COMMITS</first>
      <second type="java.lang.Boolean">true</second>
    </item>
  </setting>
  <setting name="COMMIT_TEMPLATE" type="java.lang.String">
{short description};
Issue: #{itemID};
API-modification: Y[{yes}]/N[{no}];
{internal doc}
  </setting>
  <setting name="XML_ROOT_FORMAT" type="java.lang.String">releasefab</setting>
  <setting name="LEGACY" type="java.lang.String">false</setting>
</configuration>
